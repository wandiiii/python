    "colenr = ['colenrfeb97', 'colenroct97', 'colenrfeb98','colenroct98', 'colenrfeb99', 'colenroct99', 'colenrfeb00','colenroct00', 'colenrfeb01', 'colenroct01', 'colenrfeb02',\n",
    "          'colenroct02', 'colenrfeb03', 'colenroct03', 'colenrfeb04','colenroct04', 'colenrfeb05', 'colenroct05', 'colenrfeb06','colenroct06', 'colenrfeb07', 'colenroct07', \n",
