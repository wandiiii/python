    "# 1. dtype : object > category ‚≠ê\n",
    "#   = df.dtypes Ïñ¥Ìä∏Î¶¨Î∑∞Ìä∏ \n",
    "#   + df.select_dtypes(['data type']) Î©îÏÑúÎìú\n",
    "#   + df.apply(lambda x: function) Î©îÏÑúÎìú\n",
