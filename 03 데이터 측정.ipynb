    "analysisnums = nls97.select_dtypes(include=['number'])\n",
    "analysiscats.info(), analysisnums.info()\n",
    "\n",
    "# 7. grouping columns\n",
    "demo = ['personid', 'birthmonth', 'birthyear']\n",
