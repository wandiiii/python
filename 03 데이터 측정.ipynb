    "covidtotals.set_index('iso_code', inplace=True)\n",
    "covidtotals.index # 내용, 개수\n",
    "covidtotals.index.nunique() # 종류\n",
    "covidtotals.shape # 칸수\n",
    "# 2. columns \n",
    "covidtotals.info()\n",
    "# 3. veiw sample data⭐\n",
    "covidtotals.sample(2, random_state=1).T "
